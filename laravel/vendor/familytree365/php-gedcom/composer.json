{
    "name": "familytree365/php-gedcom",
    "description": "A GEDCOM file parser (read + write) for PHP 7.3+",
    "type": "library",
    "keywords": ["gedcom","parser"],
    "homepage": "http://github.com/familytree365/php-gedcom",
    "license": "MIT",
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "Gedcom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GedcomTest\\": "tests/"
        }
    }
}
